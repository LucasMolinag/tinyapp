<header>
  <nav class="navbar navbar-expand-md navbar-dark bg-success justify-content-between">
    <a class="navbar-brand" href="/urls">TinyApp</a>
    <button
      class="navbar-toggler"
      type="button"
      data-toggle="collapse"
      data-target="#navbarNavAltMarkup"
      aria-controls="navbarNavAltMarkup"
      aria-expanded="false"
      aria-label="Toggle navigation"
    >
      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
      <div class="navbar-nav d-flex justify-content-between">
        <a class="nav-item nav-link" href="/urls">My URLs</a>
        <a class="nav-item nav-link" href="/urls/new">Create New URL</a>
      </div>
    </div>
    <div class="d-flex justify-content-right">
      <% if (typeof user === "object") { %>
        <p>Welcome, <b><%= user.email %>!</b></p>
        <form method="POST" action="/logout" class="d-flex ">
          <button type="submit" class="btn btn-secondary">Log Out</button>
        </form>
      <% } else { %>
        <a class="nav-item nav-link navbar-dark" href="/register">Register</a>
        <a class="nav-item nav-link navbar-dark" href="/login">Log In</a>
        </form>
      <% } %>
    </div>
  </nav>
</header> 